{{!
    @template core_admin/setting_qbanksetting.mustache

    Admin question bank setting template.

    Context variables required for this template:
    * name - plugin name


    Example context (json):
    {
        "name": "createcourse",
        "header": "Column sort order",
        "content-style" : "background-color: #fafafa;",
        "dd-content-style" : "",
    }
}}
<h1>
    {{header}}
</h1>
<div class="content">
        <ul class="list list-group" id="qbanklist">
            {{#name}}
                <li class="item list-group-item list-group-item-action">
                    <div>
                        <i class="fa fa-arrows"></i>
                        <span>                        
                                <i>
                                    {{.}}
                                </i>
                        </span>
                    </div>
                </li>
            {{/name}}
        </ul>
</div>